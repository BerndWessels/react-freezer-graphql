{
  "name": "gql-client",
  "version": "1.0.0",
  "description": "description",
  "author": "'BerndWessels <wesselsbernd@gmail.com>'",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/BerndWessels/gql-client.git"
  },
  "main": "index.js",
  "scripts": {
    "build": "cross-env NODE_ENV=production webpack --config webpack.config.babel.js -p --progress --colors",
    "build:serve": "babel-node --eval \"var script = require('./scripts/buildServe'); script.default().catch(err => console.error(err.stack))\"",
    "dev": "cross-env NODE_ENV=development webpack-dev-server -d --history-api-fallback --inline --progress --hot --watch",
    "externals:update": "babel-node --eval \"var script = require('./scripts/externalsUpdate'); script.default().catch(err => console.error(err.stack))\"",
    "postinstall": "npm run externals:update",
    "start": "npm run dev",
    "test": "mocha --compilers js:babel-core/register --require ./test/test_helper.js \"test/**/*.@(js|jsx)\"",
    "test:watch": "npm run test -- --watch",
    "translations:extract": "babel-node --eval \"var script = require('./scripts/translationsExtract'); script.default().catch(err => console.error(err.stack))\""
  },
  "bugs": {
    "url": "https://github.com/BerndWessels/gql-client/issues"
  },
  "homepage": "https://github.com/BerndWessels/gql-client#readme",
  "dependencies": {
    "classnames": "^2.0.0",
    "freezer-js": "^0.11.0",
    "lodash": "^4.0.0",
    "react": "^15.0.0",
    "react-dom": "^15.0.0",
    "react-intl": "^2.0.0",
    "react-router": "^2.0.0",
    "whatwg-fetch": "^1.0.0"
  },
  "devDependencies": {
    "autoprefixer": "^6.0.0",
    "babel-cli": "^6.0.0",
    "babel-loader": "^6.0.0",
    "babel-plugin-react-intl": "^2.0.0",
    "babel-plugin-react-transform": "^2.0.0",
    "babel-polyfill": "^6.0.0",
    "babel-preset-es2015": "^6.0.0",
    "babel-preset-react": "^6.0.0",
    "babel-preset-stage-0": "^6.0.0",
    "browser-sync": "^2.0.0",
    "browser-sync-webpack-plugin": "^1.0.0",
    "chai": "^3.0.0",
    "clean-webpack-plugin": "^0.1.0",
    "copy-webpack-plugin": "^3.0.0",
    "cross-env": "^1.0.0",
    "css-loader": "^0.23.0",
    "exports-loader": "^0.6.0",
    "express": "^4.13.4",
    "extract-text-webpack-plugin": "^1.0.0",
    "file-loader": "^0.8.0",
    "html-loader": "^0.4.0",
    "html-webpack-plugin": "^2.0.0",
    "imports-loader": "^0.6.0",
    "isparta-instrumenter-loader": "^1.0.0",
    "jsdom": "^9.0.0",
    "json-loader": "^0.5.0",
    "less": "^2.0.0",
    "less-loader": "^2.0.0",
    "mocha": "^2.0.0",
    "node-sass": "^3.0.0",
    "nodemon": "^1.0.0",
    "null-loader": "^0.1.0",
    "postcss-loader": "^0.9.0",
    "q": "^1.4.1",
    "raw-loader": "^0.5.0",
    "react-addons-css-transition-group": "^15.0.0",
    "react-transform-catch-errors": "^1.0.0",
    "react-transform-hmr": "^1.0.0",
    "redbox-react": "^1.0.0",
    "resolve-url-loader": "^1.0.0",
    "sass-loader": "^3.0.0",
    "style-loader": "^0.13.0",
    "webpack": "^1.0.0",
    "webpack-dev-server": "^1.0.0",
    "yargs": "^4.0.0"
  }
}
